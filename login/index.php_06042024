    <div class="modal fade sys-box-course-modal" role="dialog" tabindex="-1" id="modal-registro-talento">
        <div class="modal-dialog modal-xl modal-dialog-centered" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title">Registro Del Talento</h4><button class="btn-close" type="button" aria-label="Close" data-bs-dismiss="modal"></button>
                </div>
                <div class="modal-body sys-box-course-modal">
                    <div id="player" class="plyr__video-embed">
                        <section class="position-relative py-4 py-xl-5">
                            <div class="container position-relative">
                                <div class="row d-flex justify-content-center">
                                    <div class="col-md-8 col-lg-6 col-xl-5 col-xxl-4">
                                        <div class="card mb-5">
                                            <div class="card-body p-sm-5">
                                                <h2 class="text-center mb-4">Registrar Talento</h2>
                                                <form method="post">
                                                    <div class="mb-3"><input class="form-control form-control-lg pulse animated" type="text" data-bs-toggle="tooltip" data-bss-tooltip="" data-bss-disabled-mobile="true" id="name-2" name="name" placeholder="Nombre" autocomplete="on" required="" title="Aqui podra registrar el nombre del talento"></div>
                                                    <div class="mb-3"><input class="form-control form-control-lg shake animated" type="email" data-bs-toggle="tooltip" data-bss-tooltip="" data-bs-placement="right" id="apellido-2" name="email" placeholder="Apellidos" autocomplete="on" required="" title="Aqui podra registrar el apellido del talento"></div>
                                                    <div class="mb-3"><input class="form-control form-control-lg" type="text" data-bs-toggle="tooltip" data-bss-tooltip="" data-bs-placement="right" id="email-2" name="message" rows="6" placeholder="Correo" autocomplete="on" required="" title="Aqui podra registrar el correo electronico del talento"></div>
                                                    <div class="mb-3"><input class="form-control form-control-lg" type="password" data-bs-toggle="tooltip" data-bss-tooltip="" data-bs-placement="right" id="password-2" name="message" rows="6" placeholder="Contraseña" autocomplete="on" required="" title="Aqui podra registrar la contraseña del talento"></div>
                                                    <div class="mb-3"><input class="form-control form-control-lg" type="password" data-bs-toggle="tooltip" data-bss-tooltip="" data-bs-placement="right" id="confirmpassword-2" name="message" rows="6" placeholder="Conformar Contraseña" autocomplete="on" required="" title="Aqui podra registrar y confirmar la contraseña del talento"></div>
                                                    <div class="mb-3"><input class="form-control form-control-lg" type="text" data-bs-toggle="tooltip" data-bss-tooltip="" data-bs-placement="right" id="tipo-talento-2" name="message" rows="6" placeholder="Tipo de Talento" autocomplete="on" required="" title="Aqui podra registrar el tipo de talento del talento"></div>
                                                    <div><button class="btn btn-primary d-block w-100" type="submit">Send </button></div>
                                                </form>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </section>
                    </div>
